{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "./index.js"
        }
    ],
    "rewrites": [
        {
            "source": "/socket.io/:path*",
            "destination": "https://chit-chat-wine-eta.vercel.app/"
        }
    ]
}